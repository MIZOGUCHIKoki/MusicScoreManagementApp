    <div class = "box">
        <%= form.label :name %> <%= from.text_field :name %>
    </div>

    <div class = "box">
        <%= form.label :composer %> <%= from.text_field :composer %>
    </div>

    <div class = "box">
        <%= form.label :arranger %> <%= from.text_field :name %>
    </div>

    <div class = "box">
        <%= form.label :grade %> <%= from.text_field :name %>
    </div>

    <div class = "box">
        <%= form.label :time %> <%= from.text_field :name %>
    </div>
</div>

